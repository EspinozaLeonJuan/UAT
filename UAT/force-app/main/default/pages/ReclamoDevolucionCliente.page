    /**/
    
    function rut(idRut){
    	if(idRut.toString().trim() != '' && idRut.toString().indexOf('-') > 0){
        	var caracteres = new Array();
            var serie = new Array(2, 3, 4, 5, 6, 7);
            var dig = idRut.toString().substr(idRut.toString().length - 1, 1);
